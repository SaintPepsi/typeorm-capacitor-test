{
	"name": "typeorm-capacitor-test",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:capacitor": "cross-env VITE_ADAPTER=capacitor vite build && npx cap sync",
		"build:capacitor:android": "cross-env VITE_ADAPTER=capacitor vite build && npx cap sync android",
		"build:capacitor:ios": "cross-env VITE_ADAPTER=capacitor vite build && npx cap sync ios",
		"build:capacitor:android:run": "npm run build:capacitor:android && npx cap run android",
		"build:capacitor:ios:run": "npm run build:capacitor:ios && npx cap run ios",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/chance": "^1.1.6",
		"@types/eslint": "^8.56.0",
		"@types/node": "^20.12.3",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"terser": "^5.30.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vite-plugin-static-copy": "^1.0.2"
	},
	"type": "module",
	"dependencies": {
		"@capacitor-community/sqlite": "^5.7.0",
		"@capacitor/android": "^5.7.4",
		"@capacitor/cli": "^5.7.4",
		"@capacitor/core": "^5.7.4",
		"@capacitor/ios": "^5.7.4",
		"chance": "^1.1.11",
		"reflect-metadata": "^0.2.2",
		"typeorm": "^0.3.20"
	}
}
